var uploader = $('#<%= @uploader_id %>');

if (uploader.hasClass("gallery-uploader")) {
  $('#<%= @uploader_id %>').find('.uploader-assets-wrapper').append('<%= j render partial: "assetable/assets/asset", locals: { asset: @asset, fieldname: @fieldname } %>');
} else {
  $('#<%= @uploader_id %>').find('.uploader-assets-wrapper').html('<%= j render partial: "assetable/assets/asset", locals: { asset: @asset, fieldname: @fieldname } %>');
};